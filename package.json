{
  "name": "@enspirit/seshat",
  "version": "2.1.4",
  "description": "Seshat v2",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && tsc",
    "build:watch": "tsc -w",
    "dev": "ROOT_DIR=$PWD tsc-watch --onSuccess 'node ./dist/examples/index.js'",
    "example": "ROOT_DIR=$PWD node ./dist/examples/index.js",
    "test": "mocha -r ts-node/register tests/helpers.ts 'tests/**/*.spec.ts'",
    "test:watch": "mocha -r ts-node/register tests/helpers.ts 'tests/**/*.spec.ts' --watch --watch-files src/**/*.ts,tests/**/*.ts",
    "lint": "eslint src tests --ext ts",
    "preversion": "npm run build",
    "prepublishOnly": "npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist/src",
    "dist/package.json"
  ],
  "exports": {
    ".": "./dist/src/index.js",
    "./policies": "./dist/src/policies/index.js",
    "./transformers": "./dist/src/transformers/index.js",
    "./routers": "./dist/src/express/routers/index.js",
    "./actions": "./dist/src/actions/index.js"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.141.0",
    "@aws-sdk/lib-storage": "^3.135.0",
    "body-parser": "^1.20.0",
    "busboy": "^1.6.0",
    "express": "^4.18.1",
    "mime-types": "^2.1.35",
    "sharp": "^0.30.7",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@enspirit/eslint-config-node": "^0.1.5",
    "@types/busboy": "^1.5.0",
    "@types/chai": "^4.3.1",
    "@types/chai-as-promised": "^7.1.5",
    "@types/express": "^4.17.13",
    "@types/mime-types": "^2.1.1",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.6.1",
    "@types/sharp": "^0.30.4",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "aws-sdk-client-mock": "^1.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.20.0",
    "mocha": "^10.0.0",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "supertest": "^6.2.4",
    "ts-node": "^10.9.1",
    "ts-watch": "^1.0.8",
    "tsc-watch": "^5.0.3",
    "typescript": "^4.7.4"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  }
}
